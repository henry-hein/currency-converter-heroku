ol-12\">\n            <form onSubmit={this.handleSubmit} className=\"form-inline my-4\">\n              <h2>{chosenCurrency} Exchange Rate Table</h2>\n              <DropdownMenu passCurrency={this.chooseCurrency} />\n              <button type=\"submit\" className=\"btn btn-secondary\" style={{ width: \"100px\" }}>Submit</button>\n            </form>\n            <div className=\"table-responsive-sm\">\n              <table className=\"table\" style={{fontSize: \"16px\"}}>\n                <thead>\n                  <tr>\n                    <th scope=\"col\">Currency</th>\n                    <th scope=\"col\">Rate</th>\n                    <th scope=\"col\">Inv. Rate</th>\n                  </tr>\n                </thead>\n                <tbody>\n            {(() => {\n              if (error) {\n                return error;\n              }\n              return Object.keys(results).map((key) => {\n                return (\n                      <tr>\n                        <td>{key}</td>\n                        <td>{results[key]}</td>\n                        <td>{(1/results[key]).toFixed(4)}</td>\n                      </tr>\n                );\n              })\n            })()}\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default ExchangeRates;","/Users/Henry/Dropbox/WebDev/currency-converter-heroku/src/utils.js",[],[],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":2,"column":48,"nodeType":"55","messageId":"56","endLine":2,"endColumn":54},{"ruleId":"57","severity":1,"message":"58","line":16,"column":15,"nodeType":"59","endLine":16,"endColumn":48},{"ruleId":"57","severity":1,"message":"58","line":19,"column":15,"nodeType":"59","endLine":19,"endColumn":48},{"ruleId":"60","severity":1,"message":"61","line":29,"column":26,"nodeType":"59","messageId":"62","endLine":29,"endColumn":72,"fix":"63